{
  "number": 74,
  "name": "Al-Muddaththir",
  "script": "IndoPak",
  "ayat_count": 56,
  "ayat": [
    {
      "number": 1,
      "text": "يٰۤاَيُّهَا الۡمُدَّثِّرُۙ‏"
    },
    {
      "number": 2,
      "text": "قُمۡ فَاَنۡذِرۡۙ‏"
    },
    {
      "number": 3,
      "text": "وَرَبَّكَ فَكَبِّرۡۙ‏"
    },
    {
      "number": 4,
      "text": "وَثِيَابَكَ فَطَهِّرۡۙ‏"
    },
    {
      "number": 5,
      "text": "وَالرُّجۡزَ فَاهۡجُرۡۙ‏"
    },
    {
      "number": 6,
      "text": "وَلَا تَمۡنُنۡ تَسۡتَكۡثِرُۙ‏"
    },
    {
      "number": 7,
      "text": "وَ لِرَبِّكَ فَاصۡبِرۡؕ‏"
    },
    {
      "number": 8,
      "text": "فَاِذَا نُقِرَ فِى النَّاقُوۡرِۙ‏"
    },
    {
      "number": 9,
      "text": "فَذٰلِكَ يَوۡمَٮِٕذٍ يَّوۡمٌ عَسِيۡرٌۙ‏"
    },
    {
      "number": 10,
      "text": "عَلَى الۡكٰفِرِيۡنَ غَيۡرُ يَسِيۡرٍ‏"
    },
    {
      "number": 11,
      "text": "ذَرۡنِىۡ وَمَنۡ خَلَقۡتُ وَحِيۡدًا ۙ‏"
    },
    {
      "number": 12,
      "text": "وَّجَعَلۡتُ لَهٗ مَالًا مَّمۡدُوۡدًا ۙ‏"
    },
    {
      "number": 13,
      "text": "وَّبَنِيۡنَ شُهُوۡدًا ۙ‏"
    },
    {
      "number": 14,
      "text": "وَّمَهَّدتُّ لَهٗ تَمۡهِيۡدًا ۙ‏"
    },
    {
      "number": 15,
      "text": "ثُمَّ يَطۡمَعُ اَنۡ اَزِيۡدَ  ۙ‏"
    },
    {
      "number": 16,
      "text": "كَلَّا ؕ اِنَّهٗ كَانَ لِاٰيٰتِنَا عَنِيۡدًا ؕ‏"
    },
    {
      "number": 17,
      "text": "سَاُرۡهِقُهٗ صَعُوۡدًا ؕ‏"
    },
    {
      "number": 18,
      "text": "اِنَّهٗ فَكَّرَ وَقَدَّرَۙ‏"
    },
    {
      "number": 19,
      "text": "فَقُتِلَ كَيۡفَ قَدَّرَۙ‏"
    },
    {
      "number": 20,
      "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَۙ‏"
    },
    {
      "number": 21,
      "text": "ثُمَّ نَظَرَۙ‏"
    },
    {
      "number": 22,
      "text": "ثُمَّ عَبَسَ وَبَسَرَۙ‏"
    },
    {
      "number": 23,
      "text": "ثُمَّ اَدۡبَرَ وَاسۡتَكۡبَرَۙ‏"
    },
    {
      "number": 24,
      "text": "فَقَالَ اِنۡ هٰذَاۤ اِلَّا سِحۡرٌ يُّؤۡثَرُۙ‏"
    },
    {
      "number": 25,
      "text": "اِنۡ هٰذَاۤ اِلَّا قَوۡلُ الۡبَشَرِؕ‏"
    },
    {
      "number": 26,
      "text": "سَاُصۡلِيۡهِ سَقَرَ‏"
    },
    {
      "number": 27,
      "text": "وَمَاۤ اَدۡرٰٮكَ مَا سَقَرُؕ‏"
    },
    {
      "number": 28,
      "text": "لَا تُبۡقِىۡ وَ لَا تَذَرُ​ۚ‏"
    },
    {
      "number": 29,
      "text": "لَـوَّاحَةٌ لِّلۡبَشَرِ​ۖ​ۚ‏"
    },
    {
      "number": 30,
      "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَؕ‏"
    },
    {
      "number": 31,
      "text": "وَمَا جَعَلۡنَاۤ اَصۡحٰبَ النَّارِ اِلَّا مَلٰٓٮِٕكَةً​ وَّمَا جَعَلۡنَا عِدَّتَهُمۡ اِلَّا فِتۡنَةً لِّلَّذِيۡنَ كَفَرُوۡا ۙ لِيَسۡتَيۡقِنَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَيَزۡدَادَ الَّذِيۡنَ اٰمَنُوۡۤا اِيۡمَانًا​ وَّلَا يَرۡتَابَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَالۡمُؤۡمِنُوۡنَ​ۙ وَلِيَقُوۡلَ الَّذِيۡنَ فِىۡ قُلُوۡبِهِمۡ مَّرَضٌ وَّالۡكٰفِرُوۡنَ مَاذَاۤ اَرَادَ اللّٰهُ بِهٰذَا مَثَلًا ​ؕ كَذٰلِكَ يُضِلُّ اللّٰهُ مَنۡ يَّشَآءُ وَيَهۡدِىۡ مَنۡ يَّشَآءُ ​ؕ وَمَا يَعۡلَمُ جُنُوۡدَ رَبِّكَ اِلَّا هُوَ ​ؕ وَمَا هِىَ اِلَّا ذِكۡرٰى لِلۡبَشَرِ‏"
    },
    {
      "number": 32,
      "text": "كَلَّا وَالۡقَمَرِۙ‏"
    },
    {
      "number": 33,
      "text": "وَالَّيۡلِ اِذۡ اَدۡبَرَۙ‏"
    },
    {
      "number": 34,
      "text": "وَالصُّبۡحِ اِذَاۤ اَسۡفَرَۙ‏"
    },
    {
      "number": 35,
      "text": "اِنَّهَا لَاِحۡدَى الۡكُبَرِۙ‏"
    },
    {
      "number": 36,
      "text": "نَذِيۡرًا لِّلۡبَشَرِۙ‏"
    },
    {
      "number": 37,
      "text": "لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّتَقَدَّمَ اَوۡ يَتَاَخَّرَؕ‏"
    },
    {
      "number": 38,
      "text": "كُلُّ نَفۡسٍ ۢ بِمَا كَسَبَتۡ رَهِيۡنَةٌ ۙ‏"
    },
    {
      "number": 39,
      "text": "اِلَّاۤ اَصۡحٰبَ الۡيَمِيۡنِۛ ؕ‏"
    },
    {
      "number": 40,
      "text": "فِىۡ جَنّٰتٍ ۛ يَتَسَآءَلُوۡنَۙ‏"
    },
    {
      "number": 41,
      "text": "عَنِ الۡمُجۡرِمِيۡنَۙ‏"
    },
    {
      "number": 42,
      "text": "مَا سَلَـكَكُمۡ فِىۡ سَقَرَ‏"
    },
    {
      "number": 43,
      "text": "قَالُوۡا لَمۡ نَكُ مِنَ الۡمُصَلِّيۡنَۙ‏"
    },
    {
      "number": 44,
      "text": "وَلَمۡ نَكُ نُطۡعِمُ الۡمِسۡكِيۡنَۙ‏"
    },
    {
      "number": 45,
      "text": "وَكُنَّا نَخُوۡضُ مَعَ الۡخَـآٮِٕضِيۡنَۙ‏"
    },
    {
      "number": 46,
      "text": "وَ كُنَّا نُكَذِّبُ بِيَوۡمِ الدِّيۡنِۙ‏"
    },
    {
      "number": 47,
      "text": "حَتّٰٓى اَتٰٮنَا الۡيَقِيۡنُؕ‏"
    },
    {
      "number": 48,
      "text": "فَمَا تَنۡفَعُهُمۡ شَفَاعَةُ الشّٰفِعِيۡنَؕ‏"
    },
    {
      "number": 49,
      "text": "فَمَا لَهُمۡ عَنِ التَّذۡكِرَةِ مُعۡرِضِيۡنَۙ‏"
    },
    {
      "number": 50,
      "text": "كَاَنَّهُمۡ حُمُرٌ مُّسۡتَنۡفِرَةٌ ۙ‏"
    },
    {
      "number": 51,
      "text": "فَرَّتۡ مِنۡ قَسۡوَرَةٍ ؕ‏"
    },
    {
      "number": 52,
      "text": "بَلۡ يُرِيۡدُ كُلُّ امۡرِىٴٍ مِّنۡهُمۡ اَنۡ يُّؤۡتٰى صُحُفًا مُّنَشَّرَةً ۙ‏ ﻿﻿"
    },
    {
      "number": 53,
      "text": "كَلَّا ​ؕ بَلۡ لَّا يَخَافُوۡنَ الۡاٰخِرَةَ ؕ‏"
    },
    {
      "number": 54,
      "text": "كَلَّاۤ اِنَّهٗ تَذۡكِرَةٌ​ ۚ‏"
    },
    {
      "number": 55,
      "text": "فَمَنۡ شَآءَ ذَكَرَهٗ ؕ‏"
    },
    {
      "number": 56,
      "text": "وَمَا يَذۡكُرُوۡنَ اِلَّاۤ اَنۡ يَّشَآءَ اللّٰهُ​ ؕ هُوَ اَهۡلُ التَّقۡوٰى وَاَهۡلُ الۡمَغۡفِرَةِ‏"
    }
  ],
