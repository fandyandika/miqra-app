{
  "number": 53,
  "name": "An-Najm",
  "script": "IndoPak",
  "ayat_count": 62,
  "ayat": [
    {
      "number": 1,
      "text": "وَالنَّجۡمِ اِذَا هَوٰىۙ‏"
    },
    {
      "number": 2,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوٰى​ۚ‏"
    },
    {
      "number": 3,
      "text": "وَمَا يَنۡطِقُ عَنِ الۡهَوٰىؕ‏"
    },
    {
      "number": 4,
      "text": "اِنۡ هُوَ اِلَّا وَحۡىٌ يُّوۡحٰىۙ‏"
    },
    {
      "number": 5,
      "text": "عَلَّمَهٗ شَدِيۡدُ الۡقُوٰىۙ‏"
    },
    {
      "number": 6,
      "text": "ذُوۡ مِرَّةٍؕ فَاسۡتَوٰىۙ‏"
    },
    {
      "number": 7,
      "text": "وَهُوَ بِالۡاُفُقِ الۡاَعۡلٰى ؕ‏"
    },
    {
      "number": 8,
      "text": "ثُمَّ دَنَا فَتَدَلّٰىۙ‏"
    },
    {
      "number": 9,
      "text": "فَكَانَ قَابَ قَوۡسَيۡنِ اَوۡ اَدۡنٰى​ۚ‏"
    },
    {
      "number": 10,
      "text": "فَاَوۡحٰۤى الٰى عَبۡدِهٖ مَاۤ اَوۡحٰىؕ‏"
    },
    {
      "number": 11,
      "text": "مَا كَذَبَ الۡفُؤَادُ مَا رَاٰى‏"
    },
    {
      "number": 12,
      "text": "اَفَتُمٰرُوۡنَهٗ عَلٰى مَا يَرٰى‏"
    },
    {
      "number": 13,
      "text": "وَلَقَدۡ رَاٰهُ نَزۡلَةً اُخۡرٰىۙ‏"
    },
    {
      "number": 14,
      "text": "عِنۡدَ سِدۡرَةِ الۡمُنۡتَهٰى‏"
    },
    {
      "number": 15,
      "text": "عِنۡدَهَا جَنَّةُ الۡمَاۡوٰىؕ‏"
    },
    {
      "number": 16,
      "text": "اِذۡ يَغۡشَى السِّدۡرَةَ مَا يَغۡشٰىۙ‏"
    },
    {
      "number": 17,
      "text": "مَا زَاغَ الۡبَصَرُ وَمَا طَغٰى‏"
    },
    {
      "number": 18,
      "text": "لَقَدۡ رَاٰى مِنۡ اٰيٰتِ رَبِّهِ الۡكُبۡرٰى‏"
    },
    {
      "number": 19,
      "text": "اَفَرَءَيۡتُمُ اللّٰتَ وَالۡعُزّٰىۙ‏"
    },
    {
      "number": 20,
      "text": "وَمَنٰوةَ الثَّالِثَةَ الۡاُخۡرٰى‏"
    },
    {
      "number": 21,
      "text": "اَلَـكُمُ الذَّكَرُ وَلَهُ الۡاُنۡثٰى‏"
    },
    {
      "number": 22,
      "text": "تِلۡكَ اِذًا قِسۡمَةٌ ضِيۡزٰى‏"
    },
    {
      "number": 23,
      "text": "اِنۡ هِىَ اِلَّاۤ اَسۡمَآءٌ سَمَّيۡتُمُوۡهَاۤ اَنۡتُمۡ وَاٰبَآؤُكُمۡ مَّاۤ اَنۡزَلَ اللّٰهُ بِهَا مِنۡ سُلۡطٰنٍ​ؕ اِنۡ يَّتَّبِعُوۡنَ اِلَّا الظَّنَّ وَمَا تَهۡوَى الۡاَنۡفُسُ​ۚ وَلَقَدۡ جَآءَهُمۡ مِّنۡ رَّبِّهِمُ الۡهُدٰىؕ‏"
    },
    {
      "number": 24,
      "text": "اَمۡ لِلۡاِنۡسَانِ مَا تَمَنّٰى   ۖ‏"
    },
    {
      "number": 25,
      "text": "فَلِلّٰهِ الۡاٰخِرَةُ وَالۡاُوۡلٰى‏"
    },
    {
      "number": 26,
      "text": "وَكَمۡ مِّنۡ مَّلَكٍ فِى السَّمٰوٰتِ لَا تُغۡنِىۡ شَفَاعَتُهُمۡ شَيۡــًٔــا اِلَّا مِنۡۢ بَعۡدِ اَنۡ يَّاۡذَنَ اللّٰهُ لِمَنۡ يَّشَآءُ وَيَرۡضٰى‏"
    },
    {
      "number": 27,
      "text": "اِنَّ الَّذِيۡنَ لَا يُؤۡمِنُوۡنَ بِالۡاٰخِرَةِ لَيُسَمُّوۡنَ الۡمَلٰٓٮِٕكَةَ تَسۡمِيَةَ الۡاُنۡثٰى‏"
    },
    {
      "number": 28,
      "text": "وَمَا لَهُمۡ بِهٖ مِنۡ عِلۡمٍ​ؕ اِنۡ يَّتَّبِعُوۡنَ اِلَّا الظَّنَّ​ۚ وَاِنَّ الظَّنَّ لَا يُغۡنِىۡ مِنَ الۡحَـقِّ شَيۡـًٔـاۚ‏"
    },
    {
      "number": 29,
      "text": "فَاَعۡرِضۡ عَنۡ مَّنۡ تَوَلّٰى ۙ عَنۡ ذِكۡرِنَا وَلَمۡ يُرِدۡ اِلَّا الۡحَيٰوةَ الدُّنۡيَا ؕ‏"
    },
    {
      "number": 30,
      "text": "ذٰ لِكَ مَبۡلَـغُهُمۡ مِّنَ الۡعِلۡمِ​ ؕ اِنَّ رَبَّكَ هُوَ اَعۡلَمُ بِمَنۡ ضَلَّ عَنۡ سَبِيۡلِهٖ ۙ وَهُوَ اَعۡلَمُ بِمَنِ اهۡتَدٰى‏"
    },
    {
      "number": 31,
      "text": "وَلِلّٰهِ مَا فِى السَّمٰوٰتِ وَمَا فِى الۡاَرۡضِۙ لِيَجۡزِىَ الَّذِيۡنَ اَسَآءُوۡا بِمَا عَمِلُوۡا وَيَجۡزِىَ الَّذِيۡنَ اَحۡسَنُوۡا بِالۡحُسۡنٰى​ ۚ‏"
    },
    {
      "number": 32,
      "text": "اَلَّذِيۡنَ يَجۡتَنِبُوۡنَ كَبٰٓٮِٕرَ الۡاِثۡمِ وَالۡفوَاحِشَ اِلَّا اللَّمَمَ​ؕ اِنَّ رَبَّكَ وَاسِعُ الۡمَغۡفِرَةِ​ؕ هُوَ اَعۡلَمُ بِكُمۡ اِذۡ اَنۡشَاَكُمۡ مِّنَ الۡاَرۡضِ وَاِذۡ اَنۡتُمۡ اَجِنَّةٌ فِىۡ بُطُوۡنِ اُمَّهٰتِكُمۡ​ۚ فَلَا تُزَكُّوۡۤا اَنۡفُسَكُمۡ​ ؕ هُوَ اَعۡلَمُ بِمَنِ اتَّقٰى‏"
    },
    {
      "number": 33,
      "text": "اَفَرَءَيۡتَ الَّذِىۡ تَوَلّٰىۙ‏"
    },
    {
      "number": 34,
      "text": "وَاَعۡطٰى قَلِيۡلًا وَّاَكۡدٰى‏"
    },
    {
      "number": 35,
      "text": "اَعِنۡدَهٗ عِلۡمُ الۡغَيۡبِ فَهُوَ يَرٰى‏"
    },
    {
      "number": 36,
      "text": "اَمۡ لَمۡ يُنَبَّاۡ بِمَا فِىۡ صُحُفِ مُوۡسٰىۙ‏"
    },
    {
      "number": 37,
      "text": "وَاِبۡرٰهِيۡمَ الَّذِىۡ وَفّٰىٓ  ۙ‏"
    },
    {
      "number": 38,
      "text": "اَلَّا تَزِرُ وَازِرَةٌ وِّزۡرَ اُخۡرٰىۙ‏"
    },
    {
      "number": 39,
      "text": "وَاَنۡ لَّيۡسَ لِلۡاِنۡسَانِ اِلَّا مَا سَعٰىۙ‏"
    },
    {
      "number": 40,
      "text": "وَاَنَّ سَعۡيَهٗ سَوۡفَ يُرٰى‏"
    },
    {
      "number": 41,
      "text": "ثُمَّ يُجۡزٰٮهُ الۡجَزَآءَ الۡاَوۡفٰىۙ‏"
    },
    {
      "number": 42,
      "text": "وَاَنَّ اِلٰى رَبِّكَ الۡمُنۡتَهٰىۙ‏"
    },
    {
      "number": 43,
      "text": "وَاَنَّهٗ هُوَ اَضۡحَكَ وَاَبۡكٰىۙ‏"
    },
    {
      "number": 44,
      "text": "وَاَنَّهٗ هُوَ اَمَاتَ وَ اَحۡيَا ۙ‏"
    },
    {
      "number": 45,
      "text": "وَاَنَّهٗ خَلَقَ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰىۙ‏"
    },
    {
      "number": 46,
      "text": "مِنۡ نُّطۡفَةٍ اِذَا تُمۡنٰى‏"
    },
    {
      "number": 47,
      "text": "وَاَنَّ عَلَيۡهِ النَّشۡاَةَ الۡاُخۡرٰىۙ‏"
    },
    {
      "number": 48,
      "text": "وَاَنَّهٗ هُوَ اَغۡنٰى وَ اَقۡنٰىۙ‏"
    },
    {
      "number": 49,
      "text": "وَاَنَّهٗ هُوَ رَبُّ الشِّعۡرٰىۙ‏"
    },
    {
      "number": 50,
      "text": "وَاَنَّهٗۤ اَهۡلَكَ عَادَا۟ اۨلۡـٮُٔـوْلٰى ۙ‏"
    },
    {
      "number": 51,
      "text": "وَثَمُوۡدَا۟ فَمَاۤ اَبۡقٰىۙ‏"
    },
    {
      "number": 52,
      "text": "وَقَوۡمَ نُوۡحٍ مِّنۡ قَبۡلُ​ؕ اِنَّهُمۡ كَانُوۡا هُمۡ اَظۡلَمَ وَاَطۡغٰىؕ‏ ﻿﻿"
    },
    {
      "number": 53,
      "text": "وَالۡمُؤۡتَفِكَةَ اَهۡوٰىۙ‏"
    },
    {
      "number": 54,
      "text": "فَغَشّٰٮهَا مَا غَشّٰى​ۚ‏"
    },
    {
      "number": 55,
      "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكَ تَتَمَارٰى‏"
    },
    {
      "number": 56,
      "text": "هٰذَا نَذِيۡرٌ مِّنَ النُّذُرِ الۡاُوۡلٰٓى‏"
    },
    {
      "number": 57,
      "text": "اَزِفَتِ الۡاٰزِفَةُ​ۚ‏"
    },
    {
      "number": 58,
      "text": "لَيۡسَ لَهَا مِنۡ دُوۡنِ اللّٰهِ كَاشِفَةٌ ؕ‏"
    },
    {
      "number": 59,
      "text": "اَفَمِنۡ هٰذَا الۡحَدِيۡثِ تَعۡجَبُوۡنَۙ‏"
    },
    {
      "number": 60,
      "text": "وَتَضۡحَكُوۡنَ وَلَا تَبۡكُوۡنَۙ‏"
    },
    {
      "number": 61,
      "text": "وَاَنۡتُمۡ سٰمِدُوۡنَ‏"
    },
    {
      "number": 62,
      "text": "فَاسۡجُدُوۡا لِلّٰهِ وَاعۡبُدُوۡا ۩‏"
    }
  ],
