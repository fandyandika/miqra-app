  "ayat": [
    {
      "number": 1,
      "text": "عَمَّ يَتَسَآءَلُوۡنَ​ۚ‏"
    },
    {
      "number": 2,
      "text": "عَنِ النَّبَاِ الۡعَظِيۡمِۙ‏"
    },
    {
      "number": 3,
      "text": "الَّذِىۡ هُمۡ فِيۡهِ مُخۡتَلِفُوۡنَؕ‏"
    },
    {
      "number": 4,
      "text": "كَلَّا سَيَعۡلَمُوۡنَۙ‏"
    },
    {
      "number": 5,
      "text": "ثُمَّ كَلَّا سَيَعۡلَمُوۡنَ‏"
    },
    {
      "number": 6,
      "text": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ مِهٰدًا ۙ‏"
    },
    {
      "number": 7,
      "text": "وَّالۡجِبَالَ اَوۡتَادًا ۙ‏"
    },
    {
      "number": 8,
      "text": "وَّخَلَقۡنٰكُمۡ اَزۡوَاجًا ۙ‏"
    },
    {
      "number": 9,
      "text": "وَّجَعَلۡنَا نَوۡمَكُمۡ سُبَاتًا ۙ‏"
    },
    {
      "number": 10,
      "text": "وَّجَعَلۡنَا الَّيۡلَ لِبَاسًا ۙ‏"
    },
    {
      "number": 11,
      "text": "وَّجَعَلۡنَا النَّهَارَ مَعَاشًا‏"
    },
    {
      "number": 12,
      "text": "وَّبَنَيۡنَا فَوۡقَكُمۡ سَبۡعًا شِدَادًا ۙ‏"
    },
    {
      "number": 13,
      "text": "وَّ جَعَلۡنَا سِرَاجًا وَّهَّاجًا ۙ‏"
    },
    {
      "number": 14,
      "text": "وَّاَنۡزَلۡنَا مِنَ الۡمُعۡصِرٰتِ مَآءً ثَجَّاجًا ۙ‏"
    },
    {
      "number": 15,
      "text": "لِّـنُخۡرِجَ بِهٖ حَبًّا وَّنَبَاتًا ۙ‏"
    },
    {
      "number": 16,
      "text": "وَّجَنّٰتٍ اَلۡفَافًا ؕ‏"
    },
    {
      "number": 17,
      "text": "اِنَّ يَوۡمَ الۡفَصۡلِ كَانَ مِيۡقَاتًا ۙ‏"
    },
    {
      "number": 18,
      "text": "يَّوۡمَ يُنۡفَخُ فِى الصُّوۡرِ فَتَاۡتُوۡنَ اَفۡوَاجًا ۙ‏"
    },
    {
      "number": 19,
      "text": "وَّفُتِحَتِ السَّمَآءُ فَكَانَتۡ اَبۡوَابًا ۙ‏"
    },
    {
      "number": 20,
      "text": "وَّ سُيِّرَتِ الۡجِبَالُ فَكَانَتۡ سَرَابًا ؕ‏"
    },
    {
      "number": 21,
      "text": "اِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادًا ۙ‏"
    },
    {
      "number": 22,
      "text": "لِّلطّٰغِيۡنَ مَاٰبًا ۙ‏"
    },
    {
      "number": 23,
      "text": "لّٰبِثِيۡنَ فِيۡهَاۤ اَحۡقَابًا​ ۚ‏"
    },
    {
      "number": 24,
      "text": "لَا يَذُوۡقُوۡنَ فِيۡهَا بَرۡدًا وَّلَا شَرَابًا ۙ‏"
    },
    {
      "number": 25,
      "text": "اِلَّا حَمِيۡمًا وَّغَسَّاقًا ۙ‏"
    },
    {
      "number": 26,
      "text": "جَزَآءً وِّفَاقًا ؕ‏"
    },
    {
      "number": 27,
      "text": "اِنَّهُمۡ كَانُوۡا لَا يَرۡجُوۡنَ حِسَابًا ۙ‏"
    },
    {
      "number": 28,
      "text": "وَّكَذَّبُوۡا بِاٰيٰتِنَا كِذَّابًا ؕ‏"
    },
    {
      "number": 29,
      "text": "وَكُلَّ شَىۡءٍ اَحۡصَيۡنٰهُ كِتٰبًا ۙ‏"
    },
    {
      "number": 30,
      "text": "فَذُوۡقُوۡا فَلَنۡ نَّزِيۡدَكُمۡ اِلَّا عَذَابًا‏"
    },
    {
      "number": 31,
      "text": "اِنَّ لِلۡمُتَّقِيۡنَ مَفَازًا ۙ‏"
    },
    {
      "number": 32,
      "text": "حَدَآٮِٕقَ وَاَعۡنَابًا ۙ‏"
    },
    {
      "number": 33,
      "text": "وَّكَوَاعِبَ اَتۡرَابًا ۙ‏"
    },
    {
      "number": 34,
      "text": "وَّكَاۡسًا دِهَاقًا ؕ‏"
    },
    {
      "number": 35,
      "text": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَـغۡوًا وَّلَا كِذّٰبًا​ ۚ‏"
    },
    {
      "number": 36,
      "text": "جَزَآءً مِّنۡ رَّبِّكَ عَطَآءً حِسَابًا ۙ‏"
    },
    {
      "number": 37,
      "text": "رَّبِّ السَّمٰوٰتِ وَالۡاَرۡضِ وَمَا بَيۡنَهُمَا الرَّحۡمٰنِ​ لَا يَمۡلِكُوۡنَ مِنۡهُ خِطَابًا​ ۚ‏"
    },
    {
      "number": 38,
      "text": "يَوۡمَ يَقُوۡمُ الرُّوۡحُ وَالۡمَلٰٓٮِٕكَةُ صَفًّا ؕۙ لَّا يَتَكَلَّمُوۡنَ اِلَّا مَنۡ اَذِنَ لَهُ الرَّحۡمٰنُ وَقَالَ صَوَابًا​‏"
    },
    {
      "number": 39,
      "text": "ذٰلِكَ الۡيَوۡمُ الۡحَـقُّ​ ۚ فَمَنۡ شَآءَ اتَّخَذَ اِلٰى رَبِّهٖ مَاٰبًا‏ ﻿﻿"
    },
    {
      "number": 40,
      "text": "اِنَّاۤ اَنۡذَرۡنٰـكُمۡ عَذَابًا قَرِيۡبًا ۖۚ  يَّوۡمَ يَنۡظُرُ الۡمَرۡءُ مَا قَدَّمَتۡ يَدٰهُ وَيَقُوۡلُ الۡـكٰفِرُ يٰلَيۡتَنِىۡ كُنۡتُ تُرٰبًا‏"
    }
  ],
