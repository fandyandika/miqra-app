  "ayat": [
    {
      "number": 1,
      "text": "وَالشَّمۡسِ وَضُحٰٮهَا  ۙ‏"
    },
    {
      "number": 2,
      "text": "وَالۡقَمَرِ اِذَا تَلٰٮهَا  ۙ‏"
    },
    {
      "number": 3,
      "text": "وَالنَّهَارِ اِذَا جَلّٰٮهَا ۙ‏"
    },
    {
      "number": 4,
      "text": "وَالَّيۡلِ اِذَا يَغۡشٰٮهَا ۙ‏"
    },
    {
      "number": 5,
      "text": "وَالسَّمَآءِ وَمَا بَنٰٮهَا ۙ‏"
    },
    {
      "number": 6,
      "text": "وَالۡاَرۡضِ وَمَا طَحٰٮهَا ۙ‏"
    },
    {
      "number": 7,
      "text": "وَنَفۡسٍ وَّمَا سَوّٰٮهَا ۙ‏"
    },
    {
      "number": 8,
      "text": "فَاَلۡهَمَهَا فُجُوۡرَهَا وَتَقۡوٰٮهَا ۙ‏"
    },
    {
      "number": 9,
      "text": "قَدۡ اَفۡلَحَ مَنۡ زَكّٰٮهَا ۙ‏"
    },
    {
      "number": 10,
      "text": "وَقَدۡ خَابَ مَنۡ دَسّٰٮهَا ؕ‏"
    },
    {
      "number": 11,
      "text": "كَذَّبَتۡ ثَمُوۡدُ بِطَغۡوٰٮهَآ  ۙ‏"
    },
    {
      "number": 12,
      "text": "اِذِ انۡۢبَعَثَ اَشۡقٰٮهَا  ۙ‏"
    },
    {
      "number": 13,
      "text": "فَقَالَ لَهُمۡ رَسُوۡلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقۡيٰهَا ؕ‏"
    },
    {
      "number": 14,
      "text": "فَكَذَّبُوۡهُ فَعَقَرُوۡهَا   ۙفَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُمۡ بِذَنۡۢبِهِمۡ فَسَوّٰٮهَا  ۙ‏"
    },
    {
      "number": 15,
      "text": "وَلَا يَخَافُ عُقۡبٰهَا‏"
    }
