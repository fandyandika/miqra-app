{
  "number": 86,
  "name": "At-Tariq",
  "script": "IndoPak",
  "ayat_count": 17,
  "ayat": [
    {
      "number": 1,
      "text": "وَالسَّمَآءِ وَالطَّارِقِۙ‏"
    },
    {
      "number": 2,
      "text": "وَمَاۤ اَدۡرٰٮكَ مَا الطَّارِقُۙ‏"
    },
    {
      "number": 3,
      "text": "النَّجۡمُ الثَّاقِبُۙ‏"
    },
    {
      "number": 4,
      "text": "اِنۡ كُلُّ نَفۡسٍ لَّمَّا عَلَيۡهَا حَافِظٌؕ‏"
    },
    {
      "number": 5,
      "text": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ مِمَّ خُلِقَؕ‏"
    },
    {
      "number": 6,
      "text": "خُلِقَ مِنۡ مَّآءٍ دَافِقٍۙ‏"
    },
    {
      "number": 7,
      "text": "يَّخۡرُجُ مِنۡۢ بَيۡنِ الصُّلۡبِ وَالتَّرَآٮِٕبِؕ‏"
    },
    {
      "number": 8,
      "text": "اِنَّهٗ عَلٰى رَجۡعِهٖ لَقَادِرٌؕ‏"
    },
    {
      "number": 9,
      "text": "يَوۡمَ تُبۡلَى السَّرَآٮِٕرُۙ‏"
    },
    {
      "number": 10,
      "text": "فَمَا لَهٗ مِنۡ قُوَّةٍ وَّلَا نَاصِرٍؕ‏"
    },
    {
      "number": 11,
      "text": "وَالسَّمَآءِ ذَاتِ الرَّجۡعِۙ‏"
    },
    {
      "number": 12,
      "text": "وَالۡاَرۡضِ ذَاتِ الصَّدۡعِۙ‏"
    },
    {
      "number": 13,
      "text": "اِنَّهٗ لَقَوۡلٌ فَصۡلٌۙ‏"
    },
    {
      "number": 14,
      "text": "وَّمَا هُوَ بِالۡهَزۡلِؕ‏"
    },
    {
      "number": 15,
      "text": "اِنَّهُمۡ يَكِيۡدُوۡنَ كَيۡدًا ۙ‏"
    },
    {
      "number": 16,
      "text": "وَّاَكِيۡدُ كَيۡدًا ۚۖ‏"
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ الۡكٰفِرِيۡنَ اَمۡهِلۡهُمۡ رُوَيۡدًا‏"
    }
  ],
