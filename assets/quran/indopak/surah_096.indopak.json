  "ayat": [
    {
      "number": 1,
      "text": "اِقۡرَاۡ بِاسۡمِ رَبِّكَ الَّذِىۡ خَلَقَ​ۚ‏"
    },
    {
      "number": 2,
      "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ عَلَقٍ​ۚ‏"
    },
    {
      "number": 3,
      "text": "اِقۡرَاۡ وَرَبُّكَ الۡاَكۡرَمُۙ‏"
    },
    {
      "number": 4,
      "text": "الَّذِىۡ عَلَّمَ بِالۡقَلَمِۙ‏"
    },
    {
      "number": 5,
      "text": "عَلَّمَ الۡاِنۡسَانَ مَا لَمۡ يَعۡلَمۡؕ‏"
    },
    {
      "number": 6,
      "text": "كَلَّاۤ اِنَّ الۡاِنۡسَانَ لَيَطۡغٰٓىۙ‏"
    },
    {
      "number": 7,
      "text": "اَنۡ رَّاٰهُ اسۡتَغۡنٰىؕ‏"
    },
    {
      "number": 8,
      "text": "اِنَّ اِلٰى رَبِّكَ الرُّجۡعٰىؕ‏"
    },
    {
      "number": 9,
      "text": "اَرَءَيۡتَ الَّذِىۡ يَنۡهٰىؕ‏"
    },
    {
      "number": 10,
      "text": "عَبۡدًا اِذَا صَلّٰىؕ‏"
    },
    {
      "number": 11,
      "text": "اَرَءَيۡتَ اِنۡ كَانَ عَلَى الۡهُدٰٓىۙ‏"
    },
    {
      "number": 12,
      "text": "اَوۡ اَمَرَ بِالتَّقۡوٰىۙ‏"
    },
    {
      "number": 13,
      "text": "اَرَءَيۡتَ اِنۡ كَذَّبَ وَتَوَلّٰىؕ‏"
    },
    {
      "number": 14,
      "text": "اَلَمۡ يَعۡلَمۡ بِاَنَّ اللّٰهَ يَرٰىؕ‏"
    },
    {
      "number": 15,
      "text": "كَلَّا لَٮِٕنۡ لَّمۡ يَنۡتَهِ  ۙ لَنَسۡفَعًۢا بِالنَّاصِيَةِۙ‏"
    },
    {
      "number": 16,
      "text": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ​ ۚ‏"
    },
    {
      "number": 17,
      "text": "فَلۡيَدۡعُ نَادِيَهٗ ۙ‏"
    },
    {
      "number": 18,
      "text": "سَنَدۡعُ الزَّبَانِيَةَ ۙ‏"
    },
    {
      "number": 19,
      "text": "كَلَّا ؕ لَا تُطِعۡهُ وَاسۡجُدۡ وَاقۡتَرِبْ۩‏"
