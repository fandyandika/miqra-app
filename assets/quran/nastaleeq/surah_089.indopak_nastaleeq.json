  "ayat": [
    {
      "number": 1,
      "text": "وَالْفَجْرِ ۟ۙ"
    },
    {
      "number": 2,
      "text": "وَلَیَالٍ عَشْرٍ ۟ۙ"
    },
    {
      "number": 3,
      "text": "وَّالشَّفْعِ وَالْوَتْرِ ۟ۙ"
    },
    {
      "number": 4,
      "text": "وَالَّیْلِ اِذَا یَسْرِ ۟ۚ"
    },
    {
      "number": 5,
      "text": "هَلْ فِیْ ذٰلِكَ قَسَمٌ لِّذِیْ حِجْرٍ ۟ؕ"
    },
    {
      "number": 6,
      "text": "اَلَمْ تَرَ كَیْفَ فَعَلَ رَبُّكَ بِعَادٍ ۟"
    },
    {
      "number": 7,
      "text": "اِرَمَ ذَاتِ الْعِمَادِ ۟"
    },
    {
      "number": 8,
      "text": "الَّتِیْ لَمْ یُخْلَقْ مِثْلُهَا فِی الْبِلَادِ ۟"
    },
    {
      "number": 9,
      "text": "وَثَمُوْدَ الَّذِیْنَ جَابُوا الصَّخْرَ بِالْوَادِ ۟"
    },
    {
      "number": 10,
      "text": "وَفِرْعَوْنَ ذِی الْاَوْتَادِ ۟"
    },
    {
      "number": 11,
      "text": "الَّذِیْنَ طَغَوْا فِی الْبِلَادِ ۟"
    },
    {
      "number": 12,
      "text": "فَاَكْثَرُوْا فِیْهَا الْفَسَادَ ۟"
    },
    {
      "number": 13,
      "text": "فَصَبَّ عَلَیْهِمْ رَبُّكَ سَوْطَ عَذَابٍ ۟ۚۙ"
    },
    {
      "number": 14,
      "text": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِ ۟ؕ"
    },
    {
      "number": 15,
      "text": "فَاَمَّا الْاِنْسَانُ اِذَا مَا ابْتَلٰىهُ رَبُّهٗ فَاَكْرَمَهٗ وَنَعَّمَهٗ ۙ۬ فَیَقُوْلُ رَبِّیْۤ اَكْرَمَنِ ۟ؕ"
    },
    {
      "number": 16,
      "text": "وَاَمَّاۤ اِذَا مَا ابْتَلٰىهُ فَقَدَرَ عَلَیْهِ رِزْقَهٗ ۙ۬ فَیَقُوْلُ رَبِّیْۤ اَهَانَنِ ۟ۚ"
    },
    {
      "number": 17,
      "text": "كَلَّا بَلْ لَّا تُكْرِمُوْنَ الْیَتِیْمَ ۟ۙ"
    },
    {
      "number": 18,
      "text": "وَلَا تَحٰٓضُّوْنَ عَلٰی طَعَامِ الْمِسْكِیْنِ ۟ۙ"
    },
    {
      "number": 19,
      "text": "وَتَاْكُلُوْنَ التُّرَاثَ اَكْلًا لَّمًّا ۟ۙ"
    },
    {
      "number": 20,
      "text": "وَّتُحِبُّوْنَ الْمَالَ حُبًّا جَمًّا ۟ؕ"
    },
    {
      "number": 21,
      "text": "كَلَّاۤ اِذَا دُكَّتِ الْاَرْضُ دَكًّا دَكًّا ۟ۙ"
    },
    {
      "number": 22,
      "text": "وَّجَآءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا ۟ۚ"
    },
    {
      "number": 23,
      "text": "وَجِایْٓءَ یَوْمَىِٕذٍۭ بِجَهَنَّمَ ۙ۬ یَوْمَىِٕذٍ یَّتَذَكَّرُ الْاِنْسَانُ وَاَنّٰی لَهُ الذِّكْرٰی ۟ؕ"
    },
    {
      "number": 24,
      "text": "یَقُوْلُ یٰلَیْتَنِیْ قَدَّمْتُ لِحَیَاتِیْ ۟ۚ"
    },
    {
      "number": 25,
      "text": "فَیَوْمَىِٕذٍ لَّا یُعَذِّبُ عَذَابَهٗۤ اَحَدٌ ۟ۙ"
    },
    {
      "number": 26,
      "text": "وَّلَا یُوْثِقُ وَثَاقَهٗۤ اَحَدٌ ۟ؕ"
    },
    {
      "number": 27,
      "text": "یٰۤاَیَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُ ۟ۗۖ"
    },
    {
      "number": 28,
      "text": "ارْجِعِیْۤ اِلٰی رَبِّكِ رَاضِیَةً مَّرْضِیَّةً ۟ۚ"
    },
    {
      "number": 29,
      "text": "فَادْخُلِیْ فِیْ عِبٰدِیْ ۟ۙ"
