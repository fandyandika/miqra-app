  "ayat_count": 56,
  "ayat": [
    {
      "number": 1,
      "text": "یٰۤاَیُّهَا الْمُدَّثِّرُ ۟ۙ"
    },
    {
      "number": 2,
      "text": "قُمْ فَاَنْذِرْ ۟"
    },
    {
      "number": 3,
      "text": "وَرَبَّكَ فَكَبِّرْ ۟"
    },
    {
      "number": 4,
      "text": "وَثِیَابَكَ فَطَهِّرْ ۟"
    },
    {
      "number": 5,
      "text": "وَالرُّجْزَ فَاهْجُرْ ۟"
    },
    {
      "number": 6,
      "text": "وَلَا تَمْنُنْ تَسْتَكْثِرُ ۟"
    },
    {
      "number": 7,
      "text": "وَلِرَبِّكَ فَاصْبِرْ ۟ؕ"
    },
    {
      "number": 8,
      "text": "فَاِذَا نُقِرَ فِی النَّاقُوْرِ ۟ۙ"
    },
    {
      "number": 9,
      "text": "فَذٰلِكَ یَوْمَىِٕذٍ یَّوْمٌ عَسِیْرٌ ۟ۙ"
    },
    {
      "number": 10,
      "text": "عَلَی الْكٰفِرِیْنَ غَیْرُ یَسِیْرٍ ۟"
    },
    {
      "number": 11,
      "text": "ذَرْنِیْ وَمَنْ خَلَقْتُ وَحِیْدًا ۟ۙ"
    },
    {
      "number": 12,
      "text": "وَّجَعَلْتُ لَهٗ مَالًا مَّمْدُوْدًا ۟ۙ"
    },
    {
      "number": 13,
      "text": "وَّبَنِیْنَ شُهُوْدًا ۟ۙ"
    },
    {
      "number": 14,
      "text": "وَّمَهَّدْتُّ لَهٗ تَمْهِیْدًا ۟ۙ"
    },
    {
      "number": 15,
      "text": "ثُمَّ یَطْمَعُ اَنْ اَزِیْدَ ۟ۗۙ"
    },
    {
      "number": 16,
      "text": "كَلَّا ؕ اِنَّهٗ كَانَ لِاٰیٰتِنَا عَنِیْدًا ۟ؕ"
    },
    {
      "number": 17,
      "text": "سَاُرْهِقُهٗ صَعُوْدًا ۟ؕ"
    },
    {
      "number": 18,
      "text": "اِنَّهٗ فَكَّرَ وَقَدَّرَ ۟ۙ"
    },
    {
      "number": 19,
      "text": "فَقُتِلَ كَیْفَ قَدَّرَ ۟ۙ"
    },
    {
      "number": 20,
      "text": "ثُمَّ قُتِلَ كَیْفَ قَدَّرَ ۟ۙ"
    },
    {
      "number": 21,
      "text": "ثُمَّ نَظَرَ ۟ۙ"
    },
    {
      "number": 22,
      "text": "ثُمَّ عَبَسَ وَبَسَرَ ۟ۙ"
    },
    {
      "number": 23,
      "text": "ثُمَّ اَدْبَرَ وَاسْتَكْبَرَ ۟ۙ"
    },
    {
      "number": 24,
      "text": "فَقَالَ اِنْ هٰذَاۤ اِلَّا سِحْرٌ یُّؤْثَرُ ۟ۙ"
    },
    {
      "number": 25,
      "text": "اِنْ هٰذَاۤ اِلَّا قَوْلُ الْبَشَرِ ۟ؕ"
    },
    {
      "number": 26,
      "text": "سَاُصْلِیْهِ سَقَرَ ۟"
    },
    {
      "number": 27,
      "text": "وَمَاۤ اَدْرٰىكَ مَا سَقَرُ ۟ؕ"
    },
    {
      "number": 28,
      "text": "لَا تُبْقِیْ وَلَا تَذَرُ ۟ۚ"
    },
    {
      "number": 29,
      "text": "لَوَّاحَةٌ لِّلْبَشَرِ ۟ۚۖ"
    },
    {
      "number": 30,
      "text": "عَلَیْهَا تِسْعَةَ عَشَرَ ۟ؕ"
    },
    {
      "number": 31,
      "text": "وَمَا جَعَلْنَاۤ اَصْحٰبَ النَّارِ اِلَّا مَلٰٓىِٕكَةً ۪ وَّمَا جَعَلْنَا عِدَّتَهُمْ اِلَّا فِتْنَةً لِّلَّذِیْنَ كَفَرُوْا ۙ لِیَسْتَیْقِنَ الَّذِیْنَ اُوْتُوا الْكِتٰبَ وَیَزْدَادَ الَّذِیْنَ اٰمَنُوْۤا اِیْمَانًا وَّلَا یَرْتَابَ الَّذِیْنَ اُوْتُوا الْكِتٰبَ وَالْمُؤْمِنُوْنَ ۙ وَلِیَقُوْلَ الَّذِیْنَ فِیْ قُلُوْبِهِمْ مَّرَضٌ وَّالْكٰفِرُوْنَ مَاذَاۤ اَرَادَ اللّٰهُ بِهٰذَا مَثَلًا ؕ كَذٰلِكَ یُضِلُّ اللّٰهُ مَنْ یَّشَآءُ وَیَهْدِیْ مَنْ یَّشَآءُ ؕ وَمَا یَعْلَمُ جُنُوْدَ رَبِّكَ اِلَّا هُوَ ؕ وَمَا هِیَ اِلَّا ذِكْرٰی لِلْبَشَرِ ۟۠"
    },
    {
      "number": 32,
      "text": "كَلَّا وَالْقَمَرِ ۟ۙ"
    },
    {
      "number": 33,
      "text": "وَالَّیْلِ اِذْ اَدْبَرَ ۟ۙ"
    },
    {
      "number": 34,
      "text": "وَالصُّبْحِ اِذَاۤ اَسْفَرَ ۟ۙ"
    },
    {
      "number": 35,
      "text": "اِنَّهَا لَاِحْدَی الْكُبَرِ ۟ۙ"
    },
    {
      "number": 36,
      "text": "نَذِیْرًا لِّلْبَشَرِ ۟ۙ"
    },
    {
      "number": 37,
      "text": "لِمَنْ شَآءَ مِنْكُمْ اَنْ یَّتَقَدَّمَ اَوْ یَتَاَخَّرَ ۟ؕ"
    },
    {
      "number": 38,
      "text": "كُلُّ نَفْسٍ بِمَا كَسَبَتْ رَهِیْنَةٌ ۟ۙ"
    },
    {
      "number": 39,
      "text": "اِلَّاۤ اَصْحٰبَ الْیَمِیْنِ ۟ؕۛ"
    },
    {
      "number": 40,
      "text": "فِیْ جَنّٰتٍ ۛ۫ یَتَسَآءَلُوْنَ ۟ۙ"
    },
    {
      "number": 41,
      "text": "عَنِ الْمُجْرِمِیْنَ ۟ۙ"
    },
    {
      "number": 42,
      "text": "مَا سَلَكَكُمْ فِیْ سَقَرَ ۟"
    },
    {
      "number": 43,
      "text": "قَالُوْا لَمْ نَكُ مِنَ الْمُصَلِّیْنَ ۟ۙ"
    },
    {
      "number": 44,
      "text": "وَلَمْ نَكُ نُطْعِمُ الْمِسْكِیْنَ ۟ۙ"
    },
    {
      "number": 45,
      "text": "وَكُنَّا نَخُوْضُ مَعَ الْخَآىِٕضِیْنَ ۟ۙ"
    },
    {
      "number": 46,
      "text": "وَكُنَّا نُكَذِّبُ بِیَوْمِ الدِّیْنِ ۟ۙ"
    },
    {
      "number": 47,
      "text": "حَتّٰۤی اَتٰىنَا الْیَقِیْنُ ۟ؕ"
    },
    {
      "number": 48,
      "text": "فَمَا تَنْفَعُهُمْ شَفَاعَةُ الشّٰفِعِیْنَ ۟ؕ"
    },
    {
      "number": 49,
      "text": "فَمَا لَهُمْ عَنِ التَّذْكِرَةِ مُعْرِضِیْنَ ۟ۙ"
    },
    {
      "number": 50,
      "text": "كَاَنَّهُمْ حُمُرٌ مُّسْتَنْفِرَةٌ ۟ۙ"
    },
    {
      "number": 51,
      "text": "فَرَّتْ مِنْ قَسْوَرَةٍ ۟ؕ"
    },
    {
      "number": 52,
      "text": "بَلْ یُرِیْدُ كُلُّ امْرِئٍ مِّنْهُمْ اَنْ یُّؤْتٰی صُحُفًا مُّنَشَّرَةً ۟ۙ"
    },
    {
      "number": 53,
      "text": "كَلَّا ؕ بَلْ لَّا یَخَافُوْنَ الْاٰخِرَةَ ۟ؕ"
    },
    {
      "number": 54,
      "text": "كَلَّاۤ اِنَّهٗ تَذْكِرَةٌ ۟ۚ"
    },
    {
      "number": 55,
      "text": "فَمَنْ شَآءَ ذَكَرَهٗ ۟ؕ"
    },
    {
      "number": 56,
      "text": "وَمَا یَذْكُرُوْنَ اِلَّاۤ اَنْ یَّشَآءَ اللّٰهُ ؕ هُوَ اَهْلُ التَّقْوٰی وَاَهْلُ الْمَغْفِرَةِ ۟۠"
    }
  ],
