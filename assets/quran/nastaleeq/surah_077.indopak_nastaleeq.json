      "text": "وَالْمُرْسَلٰتِ عُرْفًا ۟ۙ"
    },
    {
      "number": 2,
      "text": "فَالْعٰصِفٰتِ عَصْفًا ۟ۙ"
    },
    {
      "number": 3,
      "text": "وَّالنّٰشِرٰتِ نَشْرًا ۟ۙ"
    },
    {
      "number": 4,
      "text": "فَالْفٰرِقٰتِ فَرْقًا ۟ۙ"
    },
    {
      "number": 5,
      "text": "فَالْمُلْقِیٰتِ ذِكْرًا ۟ۙ"
    },
    {
      "number": 6,
      "text": "عُذْرًا اَوْ نُذْرًا ۟ۙ"
    },
    {
      "number": 7,
      "text": "اِنَّمَا تُوْعَدُوْنَ لَوَاقِعٌ ۟ؕ"
    },
    {
      "number": 8,
      "text": "فَاِذَا النُّجُوْمُ طُمِسَتْ ۟ۙ"
    },
    {
      "number": 9,
      "text": "وَاِذَا السَّمَآءُ فُرِجَتْ ۟ۙ"
    },
    {
      "number": 10,
      "text": "وَاِذَا الْجِبَالُ نُسِفَتْ ۟ۙ"
    },
    {
      "number": 11,
      "text": "وَاِذَا الرُّسُلُ اُقِّتَتْ ۟ؕ"
    },
    {
      "number": 12,
      "text": "لِاَیِّ یَوْمٍ اُجِّلَتْ ۟ؕ"
    },
    {
      "number": 13,
      "text": "لِیَوْمِ الْفَصْلِ ۟ۚ"
    },
    {
      "number": 14,
      "text": "وَمَاۤ اَدْرٰىكَ مَا یَوْمُ الْفَصْلِ ۟ؕ"
    },
    {
      "number": 15,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 16,
      "text": "اَلَمْ نُهْلِكِ الْاَوَّلِیْنَ ۟ؕ"
    },
    {
      "number": 17,
      "text": "ثُمَّ نُتْبِعُهُمُ الْاٰخِرِیْنَ ۟"
    },
    {
      "number": 18,
      "text": "كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِیْنَ ۟"
    },
    {
      "number": 19,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 20,
      "text": "اَلَمْ نَخْلُقْكُّمْ مِّنْ مَّآءٍ مَّهِیْنٍ ۟ۙ"
    },
    {
      "number": 21,
      "text": "فَجَعَلْنٰهُ فِیْ قَرَارٍ مَّكِیْنٍ ۟ۙ"
    },
    {
      "number": 22,
      "text": "اِلٰی قَدَرٍ مَّعْلُوْمٍ ۟ۙ"
    },
    {
      "number": 23,
      "text": "فَقَدَرْنَا ۖۗ فَنِعْمَ الْقٰدِرُوْنَ ۟"
    },
    {
      "number": 24,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 25,
      "text": "اَلَمْ نَجْعَلِ الْاَرْضَ كِفَاتًا ۟ۙ"
    },
    {
      "number": 26,
      "text": "اَحْیَآءً وَّاَمْوَاتًا ۟ۙ"
    },
    {
      "number": 27,
      "text": "وَّجَعَلْنَا فِیْهَا رَوَاسِیَ شٰمِخٰتٍ وَّاَسْقَیْنٰكُمْ مَّآءً فُرَاتًا ۟ؕ"
    },
    {
      "number": 28,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 29,
      "text": "اِنْطَلِقُوْۤا اِلٰی مَا كُنْتُمْ بِهٖ تُكَذِّبُوْنَ ۟ۚ"
    },
    {
      "number": 30,
      "text": "اِنْطَلِقُوْۤا اِلٰی ظِلٍّ ذِیْ ثَلٰثِ شُعَبٍ ۟ۙ"
    },
    {
      "number": 31,
      "text": "لَّا ظَلِیْلٍ وَّلَا یُغْنِیْ مِنَ اللَّهَبِ ۟ؕ"
    },
    {
      "number": 32,
      "text": "اِنَّهَا تَرْمِیْ بِشَرَرٍ كَالْقَصْرِ ۟ۚ"
    },
    {
      "number": 33,
      "text": "كَاَنَّهٗ جِمٰلَتٌ صُفْرٌ ۟ؕ"
    },
    {
      "number": 34,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 35,
      "text": "هٰذَا یَوْمُ لَا یَنْطِقُوْنَ ۟ۙ"
    },
    {
      "number": 36,
      "text": "وَلَا یُؤْذَنُ لَهُمْ فَیَعْتَذِرُوْنَ ۟"
    },
    {
      "number": 37,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 38,
      "text": "هٰذَا یَوْمُ الْفَصْلِ ۚ جَمَعْنٰكُمْ وَالْاَوَّلِیْنَ ۟"
    },
    {
      "number": 39,
      "text": "فَاِنْ كَانَ لَكُمْ كَیْدٌ فَكِیْدُوْنِ ۟"
    },
    {
      "number": 40,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟۠"
    },
    {
      "number": 41,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ ظِلٰلٍ وَّعُیُوْنٍ ۟ۙ"
    },
    {
      "number": 42,
      "text": "وَّفَوَاكِهَ مِمَّا یَشْتَهُوْنَ ۟ؕ"
    },
    {
      "number": 43,
      "text": "كُلُوْا وَاشْرَبُوْا هَنِیْٓـًٔا بِمَا كُنْتُمْ تَعْمَلُوْنَ ۟"
    },
    {
      "number": 44,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟"
    },
    {
      "number": 45,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 46,
      "text": "كُلُوْا وَتَمَتَّعُوْا قَلِیْلًا اِنَّكُمْ مُّجْرِمُوْنَ ۟"
    },
    {
      "number": 47,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 48,
      "text": "وَاِذَا قِیْلَ لَهُمُ ارْكَعُوْا لَا یَرْكَعُوْنَ ۟"
    },
    {
      "number": 49,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟"
    },
    {
      "number": 50,
      "text": "فَبِاَیِّ حَدِیْثٍ بَعْدَهٗ یُؤْمِنُوْنَ ۟۠"
