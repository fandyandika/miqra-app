{
  "number": 37,
  "name": "As-Saffat",
  "script": "IndoPak-Nastaleeq",
  "ayat_count": 182,
  "ayat": [
    {
      "number": 1,
      "text": "وَالصّٰٓفّٰتِ صَفًّا ۟ۙ"
    },
    {
      "number": 2,
      "text": "فَالزّٰجِرٰتِ زَجْرًا ۟ۙ"
    },
    {
      "number": 3,
      "text": "فَالتّٰلِیٰتِ ذِكْرًا ۟ۙ"
    },
    {
      "number": 4,
      "text": "اِنَّ اِلٰهَكُمْ لَوَاحِدٌ ۟ؕ"
    },
    {
      "number": 5,
      "text": "رَبُّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَیْنَهُمَا وَرَبُّ الْمَشَارِقِ ۟ؕ"
    },
    {
      "number": 6,
      "text": "اِنَّا زَیَّنَّا السَّمَآءَ الدُّنْیَا بِزِیْنَةِ لْكَوَاكِبِ ۟ۙ"
    },
    {
      "number": 7,
      "text": "وَحِفْظًا مِّنْ كُلِّ شَیْطٰنٍ مَّارِدٍ ۟ۚ"
    },
    {
      "number": 8,
      "text": "لَا یَسَّمَّعُوْنَ اِلَی الْمَلَاِ الْاَعْلٰی وَیُقْذَفُوْنَ مِنْ كُلِّ جَانِبٍ ۟ۗۖ"
    },
    {
      "number": 9,
      "text": "دُحُوْرًا وَّلَهُمْ عَذَابٌ وَّاصِبٌ ۟ۙ"
    },
    {
      "number": 10,
      "text": "اِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَاَتْبَعَهٗ شِهَابٌ ثَاقِبٌ ۟"
    },
    {
      "number": 11,
      "text": "فَاسْتَفْتِهِمْ اَهُمْ اَشَدُّ خَلْقًا اَمْ مَّنْ خَلَقْنَا ؕ اِنَّا خَلَقْنٰهُمْ مِّنْ طِیْنٍ لَّازِبٍ ۟"
    },
    {
      "number": 12,
      "text": "بَلْ عَجِبْتَ وَیَسْخَرُوْنَ ۪۟"
    },
    {
      "number": 13,
      "text": "وَاِذَا ذُكِّرُوْا لَا یَذْكُرُوْنَ ۪۟"
    },
    {
      "number": 14,
      "text": "وَاِذَا رَاَوْا اٰیَةً یَّسْتَسْخِرُوْنَ ۪۟"
    },
    {
      "number": 15,
      "text": "وَقَالُوْۤا اِنْ هٰذَاۤ اِلَّا سِحْرٌ مُّبِیْنٌ ۟ۚۖ"
    },
    {
      "number": 16,
      "text": "ءَاِذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَ ۟ۙ"
    },
    {
      "number": 17,
      "text": "اَوَاٰبَآؤُنَا الْاَوَّلُوْنَ ۟ؕ"
    },
    {
      "number": 18,
      "text": "قُلْ نَعَمْ وَاَنْتُمْ دَاخِرُوْنَ ۟ۚ"
    },
    {
      "number": 19,
      "text": "فَاِنَّمَا هِیَ زَجْرَةٌ وَّاحِدَةٌ فَاِذَا هُمْ یَنْظُرُوْنَ ۟"
    },
    {
      "number": 20,
      "text": "وَقَالُوْا یٰوَیْلَنَا هٰذَا یَوْمُ الدِّیْنِ ۟"
    },
    {
      "number": 21,
      "text": "هٰذَا یَوْمُ الْفَصْلِ الَّذِیْ كُنْتُمْ بِهٖ تُكَذِّبُوْنَ ۟۠"
    },
    {
      "number": 22,
      "text": "اُحْشُرُوا الَّذِیْنَ ظَلَمُوْا وَاَزْوَاجَهُمْ وَمَا كَانُوْا یَعْبُدُوْنَ ۟ۙ"
    },
    {
      "number": 23,
      "text": "مِنْ دُوْنِ اللّٰهِ فَاهْدُوْهُمْ اِلٰی صِرَاطِ الْجَحِیْمِ ۟"
    },
    {
      "number": 24,
      "text": "وَقِفُوْهُمْ اِنَّهُمْ مَّسْـُٔوْلُوْنَ ۟ۙ"
    },
    {
      "number": 25,
      "text": "مَا لَكُمْ لَا تَنَاصَرُوْنَ ۟"
    },
    {
      "number": 26,
      "text": "بَلْ هُمُ الْیَوْمَ مُسْتَسْلِمُوْنَ ۟"
    },
    {
      "number": 27,
      "text": "وَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَسَآءَلُوْنَ ۟"
    },
    {
      "number": 28,
      "text": "قَالُوْۤا اِنَّكُمْ كُنْتُمْ تَاْتُوْنَنَا عَنِ الْیَمِیْنِ ۟"
    },
    {
      "number": 29,
      "text": "قَالُوْا بَلْ لَّمْ تَكُوْنُوْا مُؤْمِنِیْنَ ۟ۚ"
    },
    {
      "number": 30,
      "text": "وَمَا كَانَ لَنَا عَلَیْكُمْ مِّنْ سُلْطٰنٍ ۚ بَلْ كُنْتُمْ قَوْمًا طٰغِیْنَ ۟"
    },
    {
      "number": 31,
      "text": "فَحَقَّ عَلَیْنَا قَوْلُ رَبِّنَاۤ ۖۗ اِنَّا لَذَآىِٕقُوْنَ ۟"
    },
    {
      "number": 32,
      "text": "فَاَغْوَیْنٰكُمْ اِنَّا كُنَّا غٰوِیْنَ ۟"
    },
    {
      "number": 33,
      "text": "فَاِنَّهُمْ یَوْمَىِٕذٍ فِی الْعَذَابِ مُشْتَرِكُوْنَ ۟"
    },
    {
      "number": 34,
      "text": "اِنَّا كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِیْنَ ۟"
    },
    {
      "number": 35,
      "text": "اِنَّهُمْ كَانُوْۤا اِذَا قِیْلَ لَهُمْ لَاۤ اِلٰهَ اِلَّا اللّٰهُۙ یَسْتَكْبِرُوْنَ ۟ۙ"
    },
    {
      "number": 36,
      "text": "وَیَقُوْلُوْنَ اَىِٕنَّا لَتَارِكُوْۤا اٰلِهَتِنَا لِشَاعِرٍ مَّجْنُوْنٍ ۟ؕ"
    },
    {
      "number": 37,
      "text": "بَلْ جَآءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِیْنَ ۟"
    },
    {
      "number": 38,
      "text": "اِنَّكُمْ لَذَآىِٕقُوا الْعَذَابِ الْاَلِیْمِ ۟ۚ"
    },
    {
      "number": 39,
      "text": "وَمَا تُجْزَوْنَ اِلَّا مَا كُنْتُمْ تَعْمَلُوْنَ ۟ۙ"
    },
    {
      "number": 40,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟"
    },
    {
      "number": 41,
      "text": "اُولٰٓىِٕكَ لَهُمْ رِزْقٌ مَّعْلُوْمٌ ۟ۙ"
    },
    {
      "number": 42,
      "text": "فَوَاكِهُ ۚ وَهُمْ مُّكْرَمُوْنَ ۟ۙ"
    },
    {
      "number": 43,
      "text": "فِیْ جَنّٰتِ النَّعِیْمِ ۟ۙ"
    },
    {
      "number": 44,
      "text": "عَلٰی سُرُرٍ مُّتَقٰبِلِیْنَ ۟"
    },
    {
      "number": 45,
      "text": "یُطَافُ عَلَیْهِمْ بِكَاْسٍ مِّنْ مَّعِیْنٍ ۟ۙ"
    },
    {
      "number": 46,
      "text": "بَیْضَآءَ لَذَّةٍ لِّلشّٰرِبِیْنَ ۟ۚۖ"
    },
    {
      "number": 47,
      "text": "لَا فِیْهَا غَوْلٌ وَّلَا هُمْ عَنْهَا یُنْزَفُوْنَ ۟"
    },
    {
      "number": 48,
      "text": "وَعِنْدَهُمْ قٰصِرٰتُ الطَّرْفِ عِیْنٌ ۟ۙ"
    },
    {
      "number": 49,
      "text": "كَاَنَّهُنَّ بَیْضٌ مَّكْنُوْنٌ ۟"
    },
    {
      "number": 50,
      "text": "فَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَسَآءَلُوْنَ ۟"
    },
    {
      "number": 51,
      "text": "قَالَ قَآىِٕلٌ مِّنْهُمْ اِنِّیْ كَانَ لِیْ قَرِیْنٌ ۟ۙ"
    },
    {
      "number": 52,
      "text": "یَّقُوْلُ اَىِٕنَّكَ لَمِنَ الْمُصَدِّقِیْنَ ۟"
    },
    {
      "number": 53,
      "text": "ءَاِذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَدِیْنُوْنَ ۟"
    },
    {
      "number": 54,
      "text": "قَالَ هَلْ اَنْتُمْ مُّطَّلِعُوْنَ ۟"
    },
    {
      "number": 55,
      "text": "فَاطَّلَعَ فَرَاٰهُ فِیْ سَوَآءِ الْجَحِیْمِ ۟"
    },
    {
      "number": 56,
      "text": "قَالَ تَاللّٰهِ اِنْ كِدْتَّ لَتُرْدِیْنِ ۟ۙ"
    },
    {
      "number": 57,
      "text": "وَلَوْلَا نِعْمَةُ رَبِّیْ لَكُنْتُ مِنَ الْمُحْضَرِیْنَ ۟"
    },
    {
      "number": 58,
      "text": "اَفَمَا نَحْنُ بِمَیِّتِیْنَ ۟ۙ"
    },
    {
      "number": 59,
      "text": "اِلَّا مَوْتَتَنَا الْاُوْلٰی وَمَا نَحْنُ بِمُعَذَّبِیْنَ ۟"
    },
    {
      "number": 60,
      "text": "اِنَّ هٰذَا لَهُوَ الْفَوْزُ الْعَظِیْمُ ۟"
    },
    {
      "number": 61,
      "text": "لِمِثْلِ هٰذَا فَلْیَعْمَلِ الْعٰمِلُوْنَ ۟"
    },
    {
      "number": 62,
      "text": "اَذٰلِكَ خَیْرٌ نُّزُلًا اَمْ شَجَرَةُ الزَّقُّوْمِ ۟"
    },
    {
      "number": 63,
      "text": "اِنَّا جَعَلْنٰهَا فِتْنَةً لِّلظّٰلِمِیْنَ ۟"
    },
    {
      "number": 64,
      "text": "اِنَّهَا شَجَرَةٌ تَخْرُجُ فِیْۤ اَصْلِ الْجَحِیْمِ ۟ۙ"
    },
    {
      "number": 65,
      "text": "طَلْعُهَا كَاَنَّهٗ رُءُوْسُ الشَّیٰطِیْنِ ۟"
    },
    {
      "number": 66,
      "text": "فَاِنَّهُمْ لَاٰكِلُوْنَ مِنْهَا فَمَالِـُٔوْنَ مِنْهَا الْبُطُوْنَ ۟ؕ"
    },
    {
      "number": 67,
      "text": "ثُمَّ اِنَّ لَهُمْ عَلَیْهَا لَشَوْبًا مِّنْ حَمِیْمٍ ۟ۚ"
    },
    {
      "number": 68,
      "text": "ثُمَّ اِنَّ مَرْجِعَهُمْ لَاۡاِلَی الْجَحِیْمِ ۟"
    },
    {
      "number": 69,
      "text": "اِنَّهُمْ اَلْفَوْا اٰبَآءَهُمْ ضَآلِّیْنَ ۟ۙ"
    },
    {
      "number": 70,
      "text": "فَهُمْ عَلٰۤی اٰثٰرِهِمْ یُهْرَعُوْنَ ۟"
    },
    {
      "number": 71,
      "text": "وَلَقَدْ ضَلَّ قَبْلَهُمْ اَكْثَرُ الْاَوَّلِیْنَ ۟ۙ"
    },
    {
      "number": 72,
      "text": "وَلَقَدْ اَرْسَلْنَا فِیْهِمْ مُّنْذِرِیْنَ ۟"
    },
    {
      "number": 73,
      "text": "فَانْظُرْ كَیْفَ كَانَ عَاقِبَةُ الْمُنْذَرِیْنَ ۟ۙ"
    },
    {
      "number": 74,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟۠"
    },
    {
      "number": 75,
      "text": "وَلَقَدْ نَادٰىنَا نُوْحٌ فَلَنِعْمَ الْمُجِیْبُوْنَ ۟ؗۖ"
    },
    {
      "number": 76,
      "text": "وَنَجَّیْنٰهُ وَاَهْلَهٗ مِنَ الْكَرْبِ الْعَظِیْمِ ۟ؗۖ"
    },
    {
      "number": 77,
      "text": "وَجَعَلْنَا ذُرِّیَّتَهٗ هُمُ الْبٰقِیْنَ ۟ؗۖ"
    },
    {
      "number": 78,
      "text": "وَتَرَكْنَا عَلَیْهِ فِی الْاٰخِرِیْنَ ۟ؗۖ"
    },
    {
      "number": 79,
      "text": "سَلٰمٌ عَلٰی نُوْحٍ فِی الْعٰلَمِیْنَ ۟"
    },
    {
      "number": 80,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟"
    },
    {
      "number": 81,
      "text": "اِنَّهٗ مِنْ عِبَادِنَا الْمُؤْمِنِیْنَ ۟"
    },
    {
      "number": 82,
      "text": "ثُمَّ اَغْرَقْنَا الْاٰخَرِیْنَ ۟"
    },
    {
      "number": 83,
      "text": "وَاِنَّ مِنْ شِیْعَتِهٖ لَاِبْرٰهِیْمَ ۟ۘ"
    },
    {
      "number": 84,
      "text": "اِذْ جَآءَ رَبَّهٗ بِقَلْبٍ سَلِیْمٍ ۟"
    },
    {
      "number": 85,
      "text": "اِذْ قَالَ لِاَبِیْهِ وَقَوْمِهٖ مَاذَا تَعْبُدُوْنَ ۟ۚ"
    },
    {
      "number": 86,
      "text": "اَىِٕفْكًا اٰلِهَةً دُوْنَ اللّٰهِ تُرِیْدُوْنَ ۟ؕ"
    },
    {
      "number": 87,
      "text": "فَمَا ظَنُّكُمْ بِرَبِّ الْعٰلَمِیْنَ ۟"
    },
    {
      "number": 88,
      "text": "فَنَظَرَ نَظْرَةً فِی النُّجُوْمِ ۟ۙ"
    },
    {
      "number": 89,
      "text": "فَقَالَ اِنِّیْ سَقِیْمٌ ۟"
    },
    {
      "number": 90,
      "text": "فَتَوَلَّوْا عَنْهُ مُدْبِرِیْنَ ۟"
    },
    {
      "number": 91,
      "text": "فَرَاغَ اِلٰۤی اٰلِهَتِهِمْ فَقَالَ اَلَا تَاْكُلُوْنَ ۟ۚ"
    },
    {
      "number": 92,
      "text": "مَا لَكُمْ لَا تَنْطِقُوْنَ ۟"
    },
    {
      "number": 93,
      "text": "فَرَاغَ عَلَیْهِمْ ضَرْبًا بِالْیَمِیْنِ ۟"
    },
    {
      "number": 94,
      "text": "فَاَقْبَلُوْۤا اِلَیْهِ یَزِفُّوْنَ ۟"
    },
    {
      "number": 95,
      "text": "قَالَ اَتَعْبُدُوْنَ مَا تَنْحِتُوْنَ ۟ۙ"
    },
