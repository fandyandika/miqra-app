  "ayat_count": 46,
  "ayat": [
    {
      "number": 1,
      "text": "وَالنّٰزِعٰتِ غَرْقًا ۟ۙ"
    },
    {
      "number": 2,
      "text": "وَّالنّٰشِطٰتِ نَشْطًا ۟ۙ"
    },
    {
      "number": 3,
      "text": "وَّالسّٰبِحٰتِ سَبْحًا ۟ۙ"
    },
    {
      "number": 4,
      "text": "فَالسّٰبِقٰتِ سَبْقًا ۟ۙ"
    },
    {
      "number": 5,
      "text": "فَالْمُدَبِّرٰتِ اَمْرًا ۟ۘ"
    },
    {
      "number": 6,
      "text": "یَوْمَ تَرْجُفُ الرَّاجِفَةُ ۟ۙ"
    },
    {
      "number": 7,
      "text": "تَتْبَعُهَا الرَّادِفَةُ ۟ؕ"
    },
    {
      "number": 8,
      "text": "قُلُوْبٌ یَّوْمَىِٕذٍ وَّاجِفَةٌ ۟ۙ"
    },
    {
      "number": 9,
      "text": "اَبْصَارُهَا خَاشِعَةٌ ۟ۘ"
    },
    {
      "number": 10,
      "text": "یَقُوْلُوْنَ ءَاِنَّا لَمَرْدُوْدُوْنَ فِی الْحَافِرَةِ ۟ؕ"
    },
    {
      "number": 11,
      "text": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ۟ؕ"
    },
    {
      "number": 12,
      "text": "قَالُوْا تِلْكَ اِذًا كَرَّةٌ خَاسِرَةٌ ۟ۘ"
    },
    {
      "number": 13,
      "text": "فَاِنَّمَا هِیَ زَجْرَةٌ وَّاحِدَةٌ ۟ۙ"
    },
    {
      "number": 14,
      "text": "فَاِذَا هُمْ بِالسَّاهِرَةِ ۟ؕ"
    },
    {
      "number": 15,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ مُوْسٰی ۟ۘ"
    },
    {
      "number": 16,
      "text": "اِذْ نَادٰىهُ رَبُّهٗ بِالْوَادِ الْمُقَدَّسِ طُوًی ۟ۚ"
    },
    {
      "number": 17,
      "text": "اِذْهَبْ اِلٰی فِرْعَوْنَ اِنَّهٗ طَغٰی ۟ؗۖ"
    },
    {
      "number": 18,
      "text": "فَقُلْ هَلْ لَّكَ اِلٰۤی اَنْ تَزَكّٰی ۟ۙ"
    },
    {
      "number": 19,
      "text": "وَاَهْدِیَكَ اِلٰی رَبِّكَ فَتَخْشٰی ۟ۚ"
    },
    {
      "number": 20,
      "text": "فَاَرٰىهُ الْاٰیَةَ الْكُبْرٰی ۟ؗۖ"
    },
    {
      "number": 21,
      "text": "فَكَذَّبَ وَعَصٰی ۟ؗۖ"
    },
    {
      "number": 22,
      "text": "ثُمَّ اَدْبَرَ یَسْعٰی ۟ؗۖ"
    },
    {
      "number": 23,
      "text": "فَحَشَرَ فَنَادٰی ۟ؗۖ"
    },
    {
      "number": 24,
      "text": "فَقَالَ اَنَا رَبُّكُمُ الْاَعْلٰی ۟ؗۖ"
    },
    {
      "number": 25,
      "text": "فَاَخَذَهُ اللّٰهُ نَكَالَ الْاٰخِرَةِ وَالْاُوْلٰی ۟ؕ"
    },
    {
      "number": 26,
      "text": "اِنَّ فِیْ ذٰلِكَ لَعِبْرَةً لِّمَنْ یَّخْشٰی ۟ؕ۠"
    },
    {
      "number": 27,
      "text": "ءَاَنْتُمْ اَشَدُّ خَلْقًا اَمِ السَّمَآءُ ؕ بَنٰىهَا ۟ۙ"
    },
    {
      "number": 28,
      "text": "رَفَعَ سَمْكَهَا فَسَوّٰىهَا ۟ۙ"
    },
    {
      "number": 29,
      "text": "وَاَغْطَشَ لَیْلَهَا وَاَخْرَجَ ضُحٰىهَا ۪۟"
    },
    {
      "number": 30,
      "text": "وَالْاَرْضَ بَعْدَ ذٰلِكَ دَحٰىهَا ۟ؕ"
    },
    {
      "number": 31,
      "text": "اَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعٰىهَا ۪۟"
    },
    {
      "number": 32,
      "text": "وَالْجِبَالَ اَرْسٰىهَا ۟ۙ"
    },
    {
      "number": 33,
      "text": "مَتَاعًا لَّكُمْ وَلِاَنْعَامِكُمْ ۟ؕ"
    },
    {
      "number": 34,
      "text": "فَاِذَا جَآءَتِ الطَّآمَّةُ الْكُبْرٰی ۟ؗۖ"
    },
    {
      "number": 35,
      "text": "یَوْمَ یَتَذَكَّرُ الْاِنْسَانُ مَا سَعٰی ۟ۙ"
    },
    {
      "number": 36,
      "text": "وَبُرِّزَتِ الْجَحِیْمُ لِمَنْ یَّرٰی ۟"
    },
    {
      "number": 37,
      "text": "فَاَمَّا مَنْ طَغٰی ۟ۙ"
    },
    {
      "number": 38,
      "text": "وَاٰثَرَ الْحَیٰوةَ الدُّنْیَا ۟ۙ"
    },
    {
      "number": 39,
      "text": "فَاِنَّ الْجَحِیْمَ هِیَ الْمَاْوٰی ۟ؕ"
    },
    {
      "number": 40,
      "text": "وَاَمَّا مَنْ خَافَ مَقَامَ رَبِّهٖ وَنَهَی النَّفْسَ عَنِ الْهَوٰی ۟ۙ"
    },
    {
      "number": 41,
      "text": "فَاِنَّ الْجَنَّةَ هِیَ الْمَاْوٰی ۟ؕ"
    },
    {
      "number": 42,
      "text": "یَسْـَٔلُوْنَكَ عَنِ السَّاعَةِ اَیَّانَ مُرْسٰىهَا ۟ؕ"
    },
    {
      "number": 43,
      "text": "فِیْمَ اَنْتَ مِنْ ذِكْرٰىهَا ۟ؕ"
    },
    {
      "number": 44,
      "text": "اِلٰی رَبِّكَ مُنْتَهٰىهَا ۟ؕ"
    },
    {
      "number": 45,
      "text": "اِنَّمَاۤ اَنْتَ مُنْذِرُ مَنْ یَّخْشٰىهَا ۟ؕ"
    },
    {
      "number": 46,
      "text": "كَاَنَّهُمْ یَوْمَ یَرَوْنَهَا لَمْ یَلْبَثُوْۤا اِلَّا عَشِیَّةً اَوْ ضُحٰىهَا ۟۠"
    }
  ],
