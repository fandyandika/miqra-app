{
  "number": 55,
  "name": "Ar-Rahman",
  "script": "IndoPak-Nastaleeq",
  "ayat_count": 78,
  "ayat": [
    {
      "number": 1,
      "text": "اَلرَّحْمٰنُ ۟ۙ"
    },
    {
      "number": 2,
      "text": "عَلَّمَ الْقُرْاٰنَ ۟ؕ"
    },
    {
      "number": 3,
      "text": "خَلَقَ الْاِنْسَانَ ۟ۙ"
    },
    {
      "number": 4,
      "text": "عَلَّمَهُ الْبَیَانَ ۟"
    },
    {
      "number": 5,
      "text": "اَلشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ ۟ۙ"
    },
    {
      "number": 6,
      "text": "وَّالنَّجْمُ وَالشَّجَرُ یَسْجُدٰنِ ۟"
    },
    {
      "number": 7,
      "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ الْمِیْزَانَ ۟ۙ"
    },
    {
      "number": 8,
      "text": "اَلَّا تَطْغَوْا فِی الْمِیْزَانِ ۟"
    },
    {
      "number": 9,
      "text": "وَاَقِیْمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِیْزَانَ ۟"
    },
    {
      "number": 10,
      "text": "وَالْاَرْضَ وَضَعَهَا لِلْاَنَامِ ۟ۙ"
    },
    {
      "number": 11,
      "text": "فِیْهَا فَاكِهَةٌ وَّالنَّخْلُ ذَاتُ الْاَكْمَامِ ۟ۖ"
    },
    {
      "number": 12,
      "text": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّیْحَانُ ۟ۚ"
    },
    {
      "number": 13,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 14,
      "text": "خَلَقَ الْاِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ ۟ۙ"
    },
    {
      "number": 15,
      "text": "وَخَلَقَ الْجَآنَّ مِنْ مَّارِجٍ مِّنْ نَّارٍ ۟ۚ"
    },
    {
      "number": 16,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 17,
      "text": "رَبُّ الْمَشْرِقَیْنِ وَرَبُّ الْمَغْرِبَیْنِ ۟ۚ"
    },
    {
      "number": 18,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 19,
      "text": "مَرَجَ الْبَحْرَیْنِ یَلْتَقِیٰنِ ۟ۙ"
    },
    {
      "number": 20,
      "text": "بَیْنَهُمَا بَرْزَخٌ لَّا یَبْغِیٰنِ ۟ۚ"
    },
    {
      "number": 21,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 22,
      "text": "یَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ ۟ۚ"
    },
    {
      "number": 23,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 24,
      "text": "وَلَهُ الْجَوَارِ الْمُنْشَـٰٔتُ فِی الْبَحْرِ كَالْاَعْلَامِ ۟ۚ"
    },
    {
      "number": 25,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟۠"
    },
    {
      "number": 26,
      "text": "كُلُّ مَنْ عَلَیْهَا فَانٍ ۟ۚۖ"
    },
    {
      "number": 27,
      "text": "وَّیَبْقٰی وَجْهُ رَبِّكَ ذُو الْجَلٰلِ وَالْاِكْرَامِ ۟ۚ"
    },
    {
      "number": 28,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 29,
      "text": "یَسْـَٔلُهٗ مَنْ فِی السَّمٰوٰتِ وَالْاَرْضِ ؕ كُلَّ یَوْمٍ هُوَ فِیْ شَاْنٍ ۟ۚ"
    },
    {
      "number": 30,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 31,
      "text": "سَنَفْرُغُ لَكُمْ اَیُّهَ الثَّقَلٰنِ ۟ۚ"
    },
    {
      "number": 32,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 33,
      "text": "یٰمَعْشَرَ الْجِنِّ وَالْاِنْسِ اِنِ اسْتَطَعْتُمْ اَنْ تَنْفُذُوْا مِنْ اَقْطَارِ السَّمٰوٰتِ وَالْاَرْضِ فَانْفُذُوْا ؕ لَا تَنْفُذُوْنَ اِلَّا بِسُلْطٰنٍ ۟ۚ"
    },
    {
      "number": 34,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 35,
      "text": "یُرْسَلُ عَلَیْكُمَا شُوَاظٌ مِّنْ نَّارٍ ۙ۬ وَّنُحَاسٌ فَلَا تَنْتَصِرٰنِ ۟ۚ"
    },
    {
      "number": 36,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 37,
      "text": "فَاِذَا انْشَقَّتِ السَّمَآءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ ۟ۚ"
    },
    {
      "number": 38,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 39,
      "text": "فَیَوْمَىِٕذٍ لَّا یُسْـَٔلُ عَنْ ذَنْۢبِهٖۤ اِنْسٌ وَّلَا جَآنٌّ ۟ۚ"
    },
    {
      "number": 40,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 41,
      "text": "یُعْرَفُ الْمُجْرِمُوْنَ بِسِیْمٰهُمْ فَیُؤْخَذُ بِالنَّوَاصِیْ وَالْاَقْدَامِ ۟ۚ"
    },
    {
      "number": 42,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 43,
      "text": "هٰذِهٖ جَهَنَّمُ الَّتِیْ یُكَذِّبُ بِهَا الْمُجْرِمُوْنَ ۟ۘ"
    },
    {
      "number": 44,
      "text": "یَطُوْفُوْنَ بَیْنَهَا وَبَیْنَ حَمِیْمٍ اٰنٍ ۟ۚ"
    },
    {
      "number": 45,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟۠"
    },
    {
      "number": 46,
      "text": "وَلِمَنْ خَافَ مَقَامَ رَبِّهٖ جَنَّتٰنِ ۟ۚ"
    },
    {
      "number": 47,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۙ"
    },
    {
      "number": 48,
      "text": "ذَوَاتَاۤ اَفْنَانٍ ۟ۚ"
    },
    {
      "number": 49,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 50,
      "text": "فِیْهِمَا عَیْنٰنِ تَجْرِیٰنِ ۟ۚ"
    },
    {
      "number": 51,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 52,
      "text": "فِیْهِمَا مِنْ كُلِّ فَاكِهَةٍ زَوْجٰنِ ۟ۚ"
    },
    {
      "number": 53,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 54,
      "text": "مُتَّكِـِٕیْنَ عَلٰی فُرُشٍ بَطَآىِٕنُهَا مِنْ اِسْتَبْرَقٍ ؕ وَجَنَا الْجَنَّتَیْنِ دَانٍ ۟ۚ"
    },
    {
      "number": 55,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 56,
      "text": "فِیْهِنَّ قٰصِرٰتُ الطَّرْفِ ۙ لَمْ یَطْمِثْهُنَّ اِنْسٌ قَبْلَهُمْ وَلَا جَآنٌّ ۟ۚ"
    },
    {
      "number": 57,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۚ"
    },
    {
      "number": 58,
      "text": "كَاَنَّهُنَّ الْیَاقُوْتُ وَالْمَرْجَانُ ۟ۚ"
    },
    {
      "number": 59,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 60,
      "text": "هَلْ جَزَآءُ الْاِحْسَانِ اِلَّا الْاِحْسَانُ ۟ۚ"
    },
    {
      "number": 61,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 62,
      "text": "وَمِنْ دُوْنِهِمَا جَنَّتٰنِ ۟ۚ"
    },
    {
      "number": 63,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۙ"
    },
    {
      "number": 64,
      "text": "مُدْهَآمَّتٰنِ ۟ۚ"
    },
    {
      "number": 65,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۚ"
    },
    {
      "number": 66,
      "text": "فِیْهِمَا عَیْنٰنِ نَضَّاخَتٰنِ ۟ۚ"
    },
    {
      "number": 67,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 68,
      "text": "فِیْهِمَا فَاكِهَةٌ وَّنَخْلٌ وَّرُمَّانٌ ۟ۚ"
    },
    {
      "number": 69,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۚ"
    },
    {
      "number": 70,
      "text": "فِیْهِنَّ خَیْرٰتٌ حِسَانٌ ۟ۚ"
    },
    {
      "number": 71,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۚ"
    },
    {
      "number": 72,
      "text": "حُوْرٌ مَّقْصُوْرٰتٌ فِی الْخِیَامِ ۟ۚ"
    },
    {
      "number": 73,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۚ"
    },
    {
      "number": 74,
      "text": "لَمْ یَطْمِثْهُنَّ اِنْسٌ قَبْلَهُمْ وَلَا جَآنٌّ ۟ۚ"
    },
    {
      "number": 75,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 76,
      "text": "مُتَّكِـِٕیْنَ عَلٰی رَفْرَفٍ خُضْرٍ وَّعَبْقَرِیٍّ حِسَانٍ ۟ۚ"
    },
    {
      "number": 77,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟"
    },
    {
      "number": 78,
      "text": "تَبٰرَكَ اسْمُ رَبِّكَ ذِی الْجَلٰلِ وَالْاِكْرَامِ ۟۠"
    }
  ],
