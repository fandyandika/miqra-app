  "ayat": [
    {
      "number": 1,
      "text": "اِذَا السَّمَآءُ انْفَطَرَتْ ۟ۙ"
    },
    {
      "number": 2,
      "text": "وَاِذَا الْكَوَاكِبُ انْتَثَرَتْ ۟ۙ"
    },
    {
      "number": 3,
      "text": "وَاِذَا الْبِحَارُ فُجِّرَتْ ۟ۙ"
    },
    {
      "number": 4,
      "text": "وَاِذَا الْقُبُوْرُ بُعْثِرَتْ ۟ۙ"
    },
    {
      "number": 5,
      "text": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَاَخَّرَتْ ۟ؕ"
    },
    {
      "number": 6,
      "text": "یٰۤاَیُّهَا الْاِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِیْمِ ۟ۙ"
    },
    {
      "number": 7,
      "text": "الَّذِیْ خَلَقَكَ فَسَوّٰىكَ فَعَدَلَكَ ۟ۙ"
    },
    {
      "number": 8,
      "text": "فِیْۤ اَیِّ صُوْرَةٍ مَّا شَآءَ رَكَّبَكَ ۟ؕ"
    },
    {
      "number": 9,
      "text": "كَلَّا بَلْ تُكَذِّبُوْنَ بِالدِّیْنِ ۟ۙ"
    },
    {
      "number": 10,
      "text": "وَاِنَّ عَلَیْكُمْ لَحٰفِظِیْنَ ۟ۙ"
    },
    {
      "number": 11,
      "text": "كِرَامًا كَاتِبِیْنَ ۟ۙ"
    },
    {
      "number": 12,
      "text": "یَعْلَمُوْنَ مَا تَفْعَلُوْنَ ۟"
    },
    {
      "number": 13,
      "text": "اِنَّ الْاَبْرَارَ لَفِیْ نَعِیْمٍ ۟ۙ"
    },
    {
      "number": 14,
      "text": "وَاِنَّ الْفُجَّارَ لَفِیْ جَحِیْمٍ ۟ۚۖ"
    },
    {
      "number": 15,
      "text": "یَّصْلَوْنَهَا یَوْمَ الدِّیْنِ ۟"
    },
    {
      "number": 16,
      "text": "وَمَا هُمْ عَنْهَا بِغَآىِٕبِیْنَ ۟ؕ"
    },
    {
      "number": 17,
      "text": "وَمَاۤ اَدْرٰىكَ مَا یَوْمُ الدِّیْنِ ۟ۙ"
    },
    {
      "number": 18,
      "text": "ثُمَّ مَاۤ اَدْرٰىكَ مَا یَوْمُ الدِّیْنِ ۟ؕ"
    },
    {
      "number": 19,
      "text": "یَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَیْـًٔا ؕ وَالْاَمْرُ یَوْمَىِٕذٍ لِّلّٰهِ ۟۠"
